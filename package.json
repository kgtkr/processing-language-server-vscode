{
  "name": "processing-language-server-vscode",
  "version": "0.1.1",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "vscode:prepublish": "./generate-highlight.sh && npm run build",
    "build": "tsc",
    "build:watch": "tsc -w"
  },
  "author": "",
  "license": "GPL-3.0",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/vscode": "^1.43.0",
    "typescript": "^4.4.4",
    "vsce": "^1.100.2"
  },
  "engines": {
    "vscode": "^1.43.0"
  },
  "activationEvents": [
    "onLanguage:processing"
  ],
  "contributes": {
    "languages": [
      {
        "id": "processing",
        "aliases": [
          "Processing",
          "processing"
        ],
        "extensions": [
          ".pde"
        ]
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Example configuration",
      "properties": {
        "processing-language-server.languageServerPath": {
          "scope": "resource",
          "type": "string",
          "description": "ProcessingLanguageServer path.",
          "default": ""
        },
        "processing-language-server.processingPath": {
          "scope": "resource",
          "type": "string",
          "description": "ProcessingLanguageServer path.",
          "default": ""
        },
        "processing-language-server.autoUpdate": {
          "scope": "resource",
          "type": "boolean",
          "description": "Enable processingLanguageServer auto update.",
          "default": true
        }
      }
    },
    "commands": [
      {
        "command": "processing-language-server.restart",
        "title": "Processing: Restart Language Server"
      }
    ],
    "grammars": [
      {
        "language": "processing",
        "scopeName": "source.pde",
        "path": "processing.tmLanguage.json"
      }
    ]
  },
  "dependencies": {
    "axios": "^0.24.0",
    "vscode-languageclient": "^7.0.0"
  },
  "publisher": "kgtkr",
  "bugs": {
    "url": "https://github.com/kgtkr/processing-language-server-vscode/issues"
  },
  "homepage": "https://github.com/kgtkr/processing-language-server-vscode#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kgtkr/processing-language-server-vscode.git"
  }
}
